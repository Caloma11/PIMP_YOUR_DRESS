
<div class="register">
  <%= simple_form_for([@consultation, @review]) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.input :content, placeholder: "Type your review here", label: false, input_html: { rows: 10 } %>
    </div>

<div> Rating  : <%= rating_for @consultation, 'rating', star_path: true %></div>

<br>

    <div class="form-actions">
      <%= f.button :submit  %>
    </div>
  <% end %>
</div>



