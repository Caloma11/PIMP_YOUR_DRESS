<%= simple_form_for(current_user) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :bio %>
    <%= f.input :city %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>

<% end %>

<button type="button" class="btn btn-warning"><a href="">Not now</a></button>
